doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css')

    link(rel='stylesheet' href='https://use.fontawesome.com/releases/v5.15.3/css/all.css' integrity='sha384-SZXxX4whJ79/gErwcOYf+zWLeJdY/qpuqC4cAa9rOGUstPomtqpuNWT9wdPEn2fk' crossorigin='anonymous')

  body
    nav.navbar.navbar-expand-lg.navbar-light.bg-info
      a.navbar-brand(href='/') Robot Academy - 
        span#emailDisplayed
      button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarNav' aria-controls='navbarNav' aria-expanded='false' aria-label='Toggle navigation')
        span.navbar-toggler-icon
      #navbarNav.collapse.navbar-collapse
        ul.navbar-nav
          li.nav-item.active
            a.nav-link(href='/')
              | Home 
              span.sr-only (current)
            li.nav-item
              a#loginNavLink.nav-link(type="button") Login
            li.nav-item
              a#registerNavLink.nav-link(type="button") Register
            li.nav-item
              a#dashboardNavLink.nav-link(type="button") Dashboard
            li.nav-item
              a#logoutNavLink.nav-link(type="button") Logout
    script(src='/javascripts/labs.js')
    block content
    script(src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js")
    script(src='/javascripts/util.js')
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js')
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js')
